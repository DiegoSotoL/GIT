<div class="container p-4">
    <div class="row">
        <div class="col-md-4 mx-auto">
            <div class="card">
                <div class="cardbody">
                    <form action="/administracion/profesores/add" method="POST"autocomplete="off">
                        <div class="form-group">
                            <input type="text" name="rut" class="form-control" placeholder="Rut" autofocus>
                        </div>
                        <div class="form-group">
                            <input type="text" name="nombre_completo" class="form-control" placeholder="Nombre Completo" autofocus>
                        </div>
                        <div class="form-group">
                            <input type="text" name="telefono" class="form-control" placeholder="Telefono" autofocus>
                        </div>
                        <div class="form-group">
                            <input type="text" name="correo" class="form-control" placeholder="Correo" autofocus>
                        </div>
                        <div class="form-group">
                            <input list="browsers" class="form-control" name="id_curso" id="browser"
                                placeholder="Curso">
                            <datalist id="browsers">
                                {{#each cursos}}
                                <option value={{id}}-{{nombre_curso}}>
                                    {{/each}}
                            </datalist>
                           
                        </div>
                        <div class="form-group">
                            <button class="btn btn-success btn-block">
                                Guardar
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>